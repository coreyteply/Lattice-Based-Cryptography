                  RowBox[{"Dot", "[", 
                   RowBox[{
                    RowBox[{
                    "transMat", "\[LeftDoubleBracket]", "k", 
                    "\[RightDoubleBracket]"}], ",", 
                    RowBox[{
                    "orthomat", "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}]}], "]"}], "/", 
                  RowBox[{"Dot", "[", 
                   RowBox[{
                    RowBox[{
                    "orthomat", "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}], ",", 
                    RowBox[{
                    "orthomat", "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}]}], "]"}]}], ")"}], "]"}], "*", 
               RowBox[{
               "transMat", "\[LeftDoubleBracket]", "j", 
                "\[RightDoubleBracket]"}]}]}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"j", "=", 
             RowBox[{"j", "-", "1"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Dot", "[", 
             RowBox[{
              RowBox[{
              "orthomat", "\[LeftDoubleBracket]", "k", 
               "\[RightDoubleBracket]"}], ",", 
              RowBox[{
              "orthomat", "\[LeftDoubleBracket]", "k", 
               "\[RightDoubleBracket]"}]}], "]"}], "\[GreaterEqual]", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{".75", "-", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"Dot", "[", 
                   RowBox[{
                    RowBox[{
                    "transMat", "\[LeftDoubleBracket]", "k", 
                    "\[RightDoubleBracket]"}], ",", 
                    RowBox[{"orthomat", "\[LeftDoubleBracket]", 
                    RowBox[{"k", "-", "1"}], "\[RightDoubleBracket]"}]}], 
                   "]"}], "/", 
                  RowBox[{"Dot", "[", 
                   RowBox[{
                    RowBox[{"orthomat", "\[LeftDoubleBracket]", 
                    RowBox[{"k", "-", "1"}], "\[RightDoubleBracket]"}], ",", 
                    RowBox[{"orthomat", "\[LeftDoubleBracket]", 
                    RowBox[{"k", "-", "1"}], "\[RightDoubleBracket]"}]}], 
                   "]"}]}], ")"}], "2"]}], ")"}], "*", 
             RowBox[{"Dot", "[", 
              RowBox[{
               RowBox[{"orthomat", "\[LeftDoubleBracket]", 
                RowBox[{"k", "-", "1"}], "\[RightDoubleBracket]"}], ",", 
               RowBox[{"orthomat", "\[LeftDoubleBracket]", 
                RowBox[{"k", "-", "1"}], "\[RightDoubleBracket]"}]}], 
              "]"}]}]}], ",", "\[IndentingNewLine]", 
           RowBox[{"k", "=", 
            RowBox[{"k", "+", "1"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"tempVec", "=", 
             RowBox[{
             "transMat", "\[LeftDoubleBracket]", "k", 
              "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
             "transMat", "\[LeftDoubleBracket]", "k", 
              "\[RightDoubleBracket]"}], "=", 
             RowBox[{"transMat", "\[LeftDoubleBracket]", 
              RowBox[{"k", "-", "1"}], "\[RightDoubleBracket]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"transMat", "\[LeftDoubleBracket]", 
              RowBox[{"k", "-", "1"}], "\[RightDoubleBracket]"}], "=", 
             "tempVec"}], ";", "\[IndentingNewLine]", 
            RowBox[{"k", "=", 
             RowBox[{"Max", "[", 
              RowBox[{
               RowBox[{"k", "-", "1"}], ",", "2"}], "]"}]}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"j", "=", 
          RowBox[{"k", "-", "1"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"count", "=", 
          RowBox[{"count", "+", "1"}]}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Transpose", "[", "transMat", "]"}], ",", "count"}], 
       "}"}]}]}], "]"}]}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Returns", " ", "the", " ", "new", " ", "basis", " ", "and", " ", "the", 
