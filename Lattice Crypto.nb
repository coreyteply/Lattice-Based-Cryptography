             RowBox[{
              RowBox[{
              "transMat", "\[LeftDoubleBracket]", "k", 
               "\[RightDoubleBracket]"}], "-", 
              RowBox[{
               RowBox[{"CeilingFloor", "[", 
                RowBox[{"(", 
