Cell[BoxData[
 RowBox[{
  RowBox[{"GaussReduction2Dim", "[", "matrix_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"v1", ",", "v2", ",", "tempVec", ",", 
      RowBox[{"m", "=", "1"}], ",", 
      RowBox[{"transmat", "=", 
