     ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"2", "\[Equal]", 
        RowBox[{"Last", "[", 
         RowBox[{"Dimensions", "[", "matrix", "]"}], "]"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"While", "[", 
        RowBox[{
         RowBox[{"m", "\[NotEqual]", "0"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Norm", "[", "v2", "]"}], "<", 
             RowBox[{"Norm", "[", "v1", "]"}]}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"tempVec", "=", "v2"}], ";", "\[IndentingNewLine]", 
             RowBox[{"v2", "=", "v1"}], ";", "\[IndentingNewLine]", 
             RowBox[{"v1", "=", "tempVec"}]}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"v1", "=", "v1"}], ";", "\[IndentingNewLine]", 
             RowBox[{"v2", "=", "v2"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"m", "=", 
           RowBox[{"CeilingFloor", "[", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"Dot", "[", 
               RowBox[{"v1", ",", "v2"}], "]"}], ")"}], "/", 
             RowBox[{"Dot", "[", 
              RowBox[{"v1", ",", "v1"}], "]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"m", "\[Equal]", "0"}], ",", "\[IndentingNewLine]", " ", 
            RowBox[{
             RowBox[{"AppendTo", "[", 
              RowBox[{"goodBasis", ",", "v1"}], "]"}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"goodBasis", ",", "v2"}], "]"}]}], ",", 
